                <af:inputText value="#{bindings.ounOunName.inputValue}" id="ezfit1"/>
              </af:panelLabelAndMessage>
            </af:panelFormLayout>
            <af:outputLabel value="#{viewcontrollerBundle['sas.setting.spaOun.desc.coName']}"
