                              maximumLength="#{bindings.coName.hints.precision}"
                              shortDesc="#{bindings.coName.hints.tooltip}" id="ezfit2">
                  <f:validator binding="#{bindings.coName.validator}"/>
                </af:inputText>
              </af:panelLabelAndMessage>
              <af:panelLabelAndMessage label="#{viewcontrollerBundle['sas.setting.spaOun.coLogo']}" id="ezfplam4">
