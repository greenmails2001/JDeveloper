            <af:panelFormLayout maxColumns="2" rows="1" fieldWidth="300" labelWidth="150" id="ezfpfl1">
              <af:panelLabelAndMessage label="#{viewcontrollerBundle['field.perp.organizationUnits.ounNumber']}"
                                       id="ezfplam1">
                <af:outputLabel value="#{bindings.ounOunNumber.inputValue}" id="ezfol2"/>
              </af:panelLabelAndMessage>
              <af:panelLabelAndMessage label="#{viewcontrollerBundle['field.perp.organizationUnits.name']}"
