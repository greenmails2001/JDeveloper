<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <view id="home">
    <page>/home.jspx</page>
  </view>
  <view id="shoppingSas">
    <page>/shoppingSas.jspx</page>
  </view>
  <view id="sasAccount">
    <page>/sasAccount.jspx</page>
  </view>
  <router id="sasRouter">
    <case>
      <expression>#{ backing_sasSessionHandler.sasLogin==true}</expression>
      <outcome>shoppingSas</outcome>
    </case>
    <case>
      <expression>#{backing_sasSessionHandler.sasLogin==false}</expression>
      <outcome>login</outcome>
    </case>
    <default-outcome>login</default-outcome>
  </router>
  <view id="initialSetting">
    <page>/initialSetting.jspx</page>
  </view>
  <router id="perpRouter">
    <case>
      <expression>#{ backing_perpSessionHandler.flowCaseOfAdfConfig=="initialSetting"}</expression>
      <outcome>initialSetting</outcome>
    </case>
    <case>
      <expression>#{ backing_perpSessionHandler.flowCaseOfAdfConfig=="appView"}</expression>
      <outcome>appView</outcome>
    </case>
    <default-outcome>initialSetting</default-outcome>
  </router>
  <view id="saleAdmin">
    <page>/saleAdmin.jspx</page>
  </view>
  <view id="appView">
    <page>/appView.jspx</page>
  </view>
  <view id="helpAdmin">
    <page>/helpAdmin.jspx</page>
  </view>
  <view id="forum">
    <page>/forum.jspx</page>
  </view>
  <view id="news">
    <page>/news.jspx</page>
  </view>
  <view id="introductions">
    <page>/introductions.jspx</page>
  </view>
  <router id="adminRouter">
    <case>
      <expression>#{backing_adminFlowManager.adminFlowCase=='saleAdmin'}</expression>
      <outcome>saleAdmin</outcome>
    </case>
    <case>
      <expression>#{backing_adminFlowManager.adminFlowCase=='newsAdmin'}</expression>
      <outcome>newsAdmin</outcome>
    </case>
    <case>
      <expression>#{backing_adminFlowManager.adminFlowCase=='helpAdmin'}</expression>
      <outcome>helpAdmin</outcome>
    </case>
    <case>
      <expression>#{backing_adminFlowManager.adminFlowCase=='adminHome'}</expression>
      <outcome>adminHome</outcome>
    </case>
    <default-outcome>adminHome</default-outcome>
  </router>
  <view id="newsAdmin">
    <page>/newsAdmin.jspx</page>
  </view>
  <view id="adminHome">
    <page>/adminHome.jspx</page>
  </view>
  <view id="product">
    <page>/product.jspx</page>
  </view>
  <view id="service">
    <page>/service.jspx</page>
  </view>
  <view id="questionAnswer">
    <page>/questionAnswer.jspx</page>
  </view>
  <view id="partner">
    <page>/partner.jspx</page>
  </view>
  <view id="helpCenter">
    <page>/helpCenter.jspx</page>
  </view>
  <control-flow-rule>
    <from-activity-id>home</from-activity-id>
    <control-flow-case>
      <from-outcome>signup</from-outcome>
      <to-activity-id>sasAccount</to-activity-id>
    </control-flow-case>
    <control-flow-case>
      <from-outcome>shoppingSas</from-outcome>
      <to-activity-id>sasRouter</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule>
    <from-activity-id>sasAccount</from-activity-id>
    <control-flow-case>
      <from-outcome>shoppingSas</from-outcome>
      <to-activity-id>shoppingSas</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule>
    <from-activity-id>sasRouter</from-activity-id>
    <control-flow-case>
      <from-outcome>shoppingSas</from-outcome>
      <to-activity-id>shoppingSas</to-activity-id>
    </control-flow-case>
    <control-flow-case>
      <from-outcome>login</from-outcome>
      <to-activity-id>sasAccount</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule>
    <from-activity-id>shoppingSas</from-activity-id>
    <control-flow-case>
      <from-outcome>login</from-outcome>
      <to-activity-id>sasAccount</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule>
    <from-activity-id>perpRouter</from-activity-id>
    <control-flow-case>
      <from-outcome>initialSetting</from-outcome>
      <to-activity-id>initialSetting</to-activity-id>
    </control-flow-case>
    <control-flow-case>
      <from-outcome>appView</from-outcome>
      <to-activity-id>appView</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule>
    <from-activity-id>appView</from-activity-id>
    <control-flow-case>
      <from-outcome>initialSetting</from-outcome>
      <to-activity-id>initialSetting</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule>
    <from-activity-id>*</from-activity-id>
    <control-flow-case>
      <from-outcome>home</from-outcome>
      <to-activity-id>home</to-activity-id>
    </control-flow-case>
    <control-flow-case>
      <from-outcome>commonNews</from-outcome>
      <to-activity-id>commonNews</to-activity-id>
    </control-flow-case>
    <control-flow-case>
      <from-outcome>appView</from-outcome>
      <to-activity-id>perpRouter</to-activity-id>
    </control-flow-case>
    <control-flow-case>
      <from-outcome>sasAccount</from-outcome>
      <to-activity-id>sasRouter</to-activity-id>
    </control-flow-case>
    <control-flow-case>
      <from-outcome>helpCenter</from-outcome>
      <to-activity-id>helpCenter</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule>
    <from-activity-id>*</from-activity-id>
    <control-flow-case>
      <from-outcome>news</from-outcome>
      <to-activity-id>news</to-activity-id>
    </control-flow-case>
    <control-flow-case>
      <from-outcome>forum</from-outcome>
      <to-activity-id>forum</to-activity-id>
    </control-flow-case>
    <control-flow-case>
      <from-outcome>introduction</from-outcome>
      <to-activity-id>introductions</to-activity-id>
    </control-flow-case>
    <control-flow-case>
      <from-outcome>admin</from-outcome>
      <to-activity-id>adminRouter</to-activity-id>
    </control-flow-case>
    <control-flow-case>
      <from-outcome>product</from-outcome>
      <to-activity-id>product</to-activity-id>
    </control-flow-case>
    <control-flow-case>
      <from-outcome>service</from-outcome>
      <to-activity-id>service</to-activity-id>
    </control-flow-case>
    <control-flow-case>
      <from-outcome>consultant</from-outcome>
      <to-activity-id>questionAnswer</to-activity-id>
    </control-flow-case>
    <control-flow-case>
      <from-outcome>partner</from-outcome>
      <to-activity-id>partner</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule>
    <from-activity-id>adminRouter</from-activity-id>
    <control-flow-case>
      <from-outcome>helpAdmin</from-outcome>
      <to-activity-id>helpAdmin</to-activity-id>
    </control-flow-case>
    <control-flow-case>
      <from-outcome>saleAdmin</from-outcome>
      <to-activity-id>saleAdmin</to-activity-id>
    </control-flow-case>
    <control-flow-case>
      <from-outcome>newsAdmin</from-outcome>
      <to-activity-id>newsAdmin</to-activity-id>
    </control-flow-case>
    <control-flow-case>
      <from-outcome>adminHome</from-outcome>
      <to-activity-id>adminHome</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule>
    <from-activity-id>adminHome</from-activity-id>
    <control-flow-case>
      <from-outcome>newsAdmin</from-outcome>
      <to-activity-id>newsAdmin</to-activity-id>
    </control-flow-case>
    <control-flow-case>
      <from-outcome>helpAdmin</from-outcome>
      <to-activity-id>helpAdmin</to-activity-id>
    </control-flow-case>
    <control-flow-case>
      <from-outcome>saleAdmin</from-outcome>
      <to-activity-id>saleAdmin</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule>
    <from-activity-id>saleAdmin</from-activity-id>
    <control-flow-case>
      <from-outcome>newsAdmin</from-outcome>
      <to-activity-id>newsAdmin</to-activity-id>
    </control-flow-case>
    <control-flow-case>
      <from-outcome>helpAdmin</from-outcome>
      <to-activity-id>helpAdmin</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule>
    <from-activity-id>helpAdmin</from-activity-id>
    <control-flow-case>
      <from-outcome>newAdmin</from-outcome>
      <to-activity-id>newsAdmin</to-activity-id>
    </control-flow-case>
    <control-flow-case>
      <from-outcome>saleAdmin</from-outcome>
      <to-activity-id>saleAdmin</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule>
    <from-activity-id>newsAdmin</from-activity-id>
    <control-flow-case>
      <from-outcome>saleAdmin</from-outcome>
      <to-activity-id>saleAdmin</to-activity-id>
    </control-flow-case>
    <control-flow-case>
      <from-outcome>helpAdmin</from-outcome>
      <to-activity-id>helpAdmin</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <managed-bean>
    <managed-bean-name>backing_newsAdmin_newsAdminManager</managed-bean-name>
    <managed-bean-class>com.vc.web.backing.newsAdmin.NewsAdminManager</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
