<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="help-center-flow">
    <default-activity>helpViewer</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean>
      <managed-bean-name>backing_helpCenter_helpCenterManager</managed-bean-name>
      <managed-bean-class>com.vc.web.backing.helpCenter.HelpCenterManager</managed-bean-class>
      <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <view id="helpViewer">
      <page>/pages/fragments/helpCenter/helpViewer.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
